      </restriction>
    </simpleType>
  </element>
  <simpleType name="ShortNavnTekst" oio:definition="urn:oio:sts:shortnavn:BDD:6">
    <restriction base="string">
      <maxLength value="100"/>
    </restriction>
  </simpleType>
  <element name="AdresseTekst" oio:definition="urn:oio:sts:email:BDD:6">
    <simpleType>
      <restriction base="string">
        <maxLength value="200"/>
      </restriction>
    </simpleType>
  </element>
  <element name="MyndighedsKode" oio:definition="urn:oio:sts:email:BDD:6">
    <simpleType>
      <restriction base="string">
        <pattern value="[0-9]{3}|[0-9]{4}"/>
      </restriction>
    </simpleType>
  </element>
</schema>